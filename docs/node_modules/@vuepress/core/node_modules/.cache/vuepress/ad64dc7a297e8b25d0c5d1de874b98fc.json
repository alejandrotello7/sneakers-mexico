\n      dates: [],\n      times:[],\n      checked: {\n        brand: [],\n      },\n      brands: [\n        \"Nike\",\n        \"Puma\",\n        \"Reebok\",\n        \"Adidas\",\n        \"Converse\",\n\n      ],\n      trigger: false,\n    }\n  },\n  computed: {\n    lanzamientos() {\n      return this.$site.pages\n          .filter(\n              (x) =>\n                  x.path.startsWith(\"/lanzamientos/\") &&\n                  !x.frontmatter.lanzamientos_index\n          )\n          .sort(\n              (a, b) => new Date(a.frontmatter.date) - new Date(b.frontmatter.date)\n          );\n    },\n    formateoDate() {\n      const options = {\n        weekday: \"long\",\n        month: \"long\",\n        day: \"numeric\",\n      };\n      this.$site.pages.map((page) => {\n        this.pagesArray.push(page)\n      });\n\n      //Filter launch sneakers\n      this.pagesArray.forEach(page => {\n        if( page.path.startsWith(\"/lanzamientos/\") && !page.frontmatter.lanzamientos_index){\n          this.dates.push(page.frontmatter.date)\n        }\n      })\n\n      //Sort by date\n      this.dates.sort((a,b)=>\n          new Date(a) - new Date(b));\n\n      //Convert to Readible Date and Time\n      this.dates.forEach((date, index) => {\n        this.dates[index] = new Date(date).toLocaleDateString('es-mx',options)\n        this.times.push(new Date(date).toLocaleTimeString().replace(/(.*)\\D\\d+/, '$1'))\n      })\n\n      console.log(this.dates)\n    },\n  },\n  methods:{\n\n  },\n  beforeMount(){\n    this.formateoDate\n  },\n};\n"],"mappings":";AACA,OAAOA,iBAAiB,MAAM,yBAAyB;AACvD,OAAOC,MAAM,MAAM,2CAA2C;AAC9D,OAAOC,SAAS,MAAM,aAAa;AAEnC,eAAe;EACbC,UAAU,EAAE;IACVH,iBAAiB;IACjBC,MAAM;IACNC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,UAAU,EAAE,EAAE;MACdC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAC,EAAE;MACRC,OAAO,EAAE;QACPC,KAAK,EAAE;MACT,CAAC;MACDC,MAAM,EAAE,CACN,MAAM,EACN,MAAM,EACN,QAAQ,EACR,QAAQ,EACR,UAAU,CAEX;MACDC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC